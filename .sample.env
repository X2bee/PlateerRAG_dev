# .env file for the project
# Please ensure to keep this file secure and do not expose sensitive information in public repositories.

# Common environment variables used in the project
OPENAI_API_KEY=
DEBUG_MODE=true
VLLM_MODEL_NAME=Qwen/Qwen3-14B

# Database configuration
DATABASE_TYPE=postgresql
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_USER=ailab
POSTGRES_PASSWORD=ailab123
POSTGRES_DB=plateerag

# Qdrant configuration
QDRANT_HOST=qdrant
QDRANT_HTTP_PORT=6333
QDRANT_GRPC_PORT=6334
QDRANT_CONFIG_PATH=/qdrant/config/production.yaml

# Frontend configuration
NEXT_PUBLIC_BACKEND_HOST=http://localhost
NEXT_PUBLIC_BACKEND_PORT=8000

# Vast proxy configuration
VAST_PROXY_MODE=proxy
VAST_PROXY_BASE_URL=http://host.docker.internal:8024
VAST_PROXY_TIMEOUT=300
VAST_PROXY_API_TOKEN=

MLFLOW_URL=MINIO_URL

MLFLOW_DEFAULT_ARTIFACT_ROOT=s3://mlflow-artifacts

# MinIO (S3 호환)
MLFLOW_S3_ENDPOINT_URL=S3_ENDPOINT
AWS_ACCESS_KEY_ID=ACCESS_ID
AWS_SECRET_ACCESS_KEY=ACCESS_KEY
AWS_DEFAULT_REGION=REGION
# Model Registry
ENABLE_MODEL_REGISTRY=true

# Backend configuration
NEXT_PUBLIC_FRONTEND_HOST=FRONT_HOST
NEXT_PUBLIC_FRONTEND_PORT=FRONT_PORT

FRONTEND_RUNNER_TOKEN=GITACTIONTOKEN
BACKEND_RUNNER_TOKEN=ITACTIONTOKEN
